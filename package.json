    "test:rust": "cd wasm && cargo test",
    "demo": "cd demo && npm run dev",
    "bench": "cd bench && npm run bench",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "csv",
    "parser",
    "wasm",
    "rust",
    "typescript",
    "streaming",
    "high-performance"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-wasm": "^6.0.0",
    "rollup": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "@types/node": "^20.0.0"
  },
  "files": [
    "dist",
    "wasm/pkg"
  ]
}